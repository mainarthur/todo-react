{
  "extends": [
    "airbnb-typescript",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "only-warn"
  ],
  "rules": {
    "@typescript-eslint/semi": 0,
    "semi": [
      2,
      "never"
    ],
    "import/prefer-default-export": 0,
    "import/no-cycle": [
      2,
      {
        "maxDepth": 2
      }
    ]
  }
}